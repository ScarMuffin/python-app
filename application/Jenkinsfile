node {
    checkout scm

    docker.withRegistry('https://sashok.jfrog.io/default-docker-virtual/', 'jfrog') {

       def customImage = docker.build("my-image:${env.BUILD_ID}", "-f Dockerfile ./application")

        /* Push the container to the custom Registry */
        customImage.push()
    }
}

